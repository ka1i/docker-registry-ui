{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "/^": [
        "."
      ],
      "/^/*": [
        "./*"
      ],
      "/@": [
        "src"
      ],
      "/@/*": [
        "src/*"
      ],
      "/#": [
        "types"
      ],
      "/#/*": [
        "types/*"
      ],
    },
  },
  "include": [
    "src/*.vue",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/**/*.ts",
    "src/**/**/*.vue"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}